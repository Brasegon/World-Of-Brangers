; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="World of Brangers"
run/main_scene="res://Scenes/Global.tscn"

[autoload]

Logger="*res://Librairies/Logger/logger.gd"
Network="*res://Modules/Utils/Network/Network.tscn"
Global="*res://Scenes/Global.tscn"
MainMenu="*res://Modules/Menu/MainMenu/MainMenu.tscn"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"detect_3d": false,
"flags/filter": false,
"process/fix_alpha_border": false
}

[layer_names]

2d_render/layer_1="Environnement"
2d_render/layer_2="Player"
2d_physics/layer_1="Environnement"
2d_physics/layer_2="Player"

[physics]

common/enable_pause_aware_picking=true
